		<version>3.0-SNAPSHOT</version>
		<relativePath>../parent-poms/microservice</relativePath>
	</parent>
	<modelVersion>4.0.0</modelVersion>
	<groupId>org.eea.dataset</groupId>
	<artifactId>dataset-service</artifactId>
	<version>${dataset.version}</version>
	<properties>
		<sonar.moduleKey>org.eea:reportnet:dataset-service:${sonar.jenkins.branch}</sonar.moduleKey>
	</properties>
	<dependencies>
		<dependency>
			<groupId>com.opencsv</groupId>
